{
    "description": "Swipes right on 100 of your loaded tinder matches. Requires you to open https://tinder.com Signs you in via Facebook",
    "manifest_version": 2,
    "name": "Tinder Right Swipe",
    "version": "1.0",
    "homepage_url": "https://github.com/anknits/Swipe.git",
    "icons": {
      "16": "icons/swipe-icon-16.png",
      "32": "icons/swipe-icon-32.png",
      "64": "icons/swipe-icon-64.png",
      "128": "icons/swipe-icon-128.png",
      "256": "icons/swipe-icon-256.png"
    },
    "permissions": [
      "*://*.tinder.com/*", "*://*.facebook.com/*", "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.tinder.com/*", "*://*.facebook.com/*"],
        "js": ["swipe-full.js"]
      }
    ],
    "user_scripts": {}      
  }